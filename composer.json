{
    "name": "grizzlyware/aero-rich-additionals",
    "description": "Allow recording structured additional attributes against models in Aero Commerce",
    "type": "library",
    "require": {
        "php": "^8.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Grizzlyware\\Aero\\RichAdditionals\\": "./src/"
        }
    },
    "authors": [
        {
            "name": "Grizzlyware Ltd",
            "email": "contact@grizzlyware.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Grizzlyware\\Aero\\RichAdditionals\\Providers\\RichAdditionalsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://agora.aerocommerce.com"
        }
    ],
    "require-dev": {
        "aerocommerce/admin": "^1.24"
    }
}
